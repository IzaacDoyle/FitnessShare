<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/F_FS_Splash"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.501"
            android:layout_marginBottom="644dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/S_FS_Splash"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="52dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginBottom="364dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/Itness_FS_Splash"
            android:id="@+id/Hare_FS_Splash"
            android:visibility="invisible"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="1.0" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginTop="252dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/Itness_FS_Splash"
            android:visibility="invisible"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="1.0"></Constraint>
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="52dp"
            android:layout_marginStart="52dp"
            android:id="@+id/F_FS_Splash"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.456" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/F_FS_Splash"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/Itness_FS_Splash"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.456" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/S_FS_Splash"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/Hare_FS_Splash"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.456" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="240dp"
            android:layout_marginStart="240dp"
            android:id="@+id/S_FS_Splash"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.456" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/middle" >
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/S_FS_Splash"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/Hare_FS_Splash"
            android:visibility="visible"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.46" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/F_FS_Splash"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/Itness_FS_Splash"
            android:visibility="visible"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.376" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="196dp"
            android:layout_marginStart="196dp"
            android:id="@+id/F_FS_Splash"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.376" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="196dp"
            android:layout_marginStart="196dp"
            android:id="@+id/S_FS_Splash"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.459" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/middleEnd" >
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginTop="252dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="76dp"
            android:layout_marginStart="76dp"
            android:id="@+id/F_FS_Splash"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="423dp"
            motion:layout_constraintVertical_bias="1.0" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="240dp"
            android:layout_marginStart="240dp"
            android:id="@+id/S_FS_Splash"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.456" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/S_FS_Splash"
            android:id="@+id/Hare_FS_Splash"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.456" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@+id/F_FS_Splash"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="252dp"
            android:id="@+id/Itness_FS_Splash"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="423dp"
            motion:layout_constraintVertical_bias="0.0" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/middle"
        motion:constraintSetEnd="@+id/middleEnd"
        motion:autoTransition="animateToEnd"
        motion:motionInterpolator="cubic(0.28,0.26,0.78,0.64)" />
    <Transition
        motion:constraintSetStart="@+id/start"
        motion:constraintSetEnd="@+id/middle"
        motion:autoTransition="animateToEnd" />
    <Transition
        motion:constraintSetStart="@+id/middleEnd"
        motion:constraintSetEnd="@+id/end"
        motion:autoTransition="animateToEnd" />
</MotionScene>